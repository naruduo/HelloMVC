<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.Notice" table="notice" schema="se_web">
        <id name="notificationId">
            <column name="notification_id" sql-type="int(10)"/>
        </id>
        <property name="notiContent">
            <column name="noti_content" sql-type="varchar(255)"/>
        </property>
        <property name="releaserId">
            <column name="releaser_id" sql-type="int(10)"/>
        </property>
        <property name="releaseTime">
            <column name="release_time" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>