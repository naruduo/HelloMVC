<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.TestScore" table="test_score" schema="se_web">
        <id name="stuId">
            <column name="stu_id" sql-type="int(10)"/>
        </id>
        <property name="grade">
            <column name="grade" sql-type="int(10)" not-null="true"/>
        </property>
        <property name="time">
            <column name="time" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>