<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.ExpDoc" table="exp_doc" schema="se_web">
        <id name="expDocId">
            <column name="exp_doc_id" sql-type="int(10)"/>
        </id>
        <property name="stuId">
            <column name="stu_id" sql-type="int(10)"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(255)"/>
        </property>
        <property name="subTime">
            <column name="sub_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="score">
            <column name="score" sql-type="int(10)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>